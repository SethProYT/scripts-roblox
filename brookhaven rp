if game.placeId == 4924922222 then

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Brookhaven RP", "Sentinel")

local Tab1 = Window:NewTab("Character")
local Tab2 = Window:NewTab("Rob")
local Tab3 = Window:NewTab("Main")

local Section1 = Tab1:NewSection("Scripts")
local Section2 = Tab2:NewSection("Scripts")
local Section3 = Tab3:NewSection("Scripts")


Section3:NewButton("Break Scooter", "", function()
    -- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "NoMotorVehicleDeleteCar"
}

game:GetService("ReplicatedStorage").RemoteEvents.C5ar:FireServer(unpack(args))

-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "Scooter"
}

game:GetService("ReplicatedStorage").RemoteEvents.NoMotorVehicles:FireServer(unpack(args))
end)


Section2:NewButton("Rob Bank", "", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-7.9449858665466, 17.698003768921, 258.89273071289)

   wait(1)
   -- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "PickingTools",
    [2] = "Bomb"
}

game:GetService("ReplicatedStorage").RemoteEvents.T5ools:InvokeServer(unpack(args))
wait(1)

for _, tool in ipairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
    if tool:IsA("Tool") then
         tool.Parent = game:GetService("Players").LocalPlayer.Character -- I didn't use Equip because the Equip function unequips any other tools in your character.
    end
end

local StarterGui = game:GetService("StarterGui")

local Notifications = {
	"Now plant the bomb and just rob it!",
	
}

local TimeBetweenNotifications = 1 --REPLACE 5 WITH THE NUMBER OF SECONDS YOU WANT TO WAIT IN BETWEEN NOTIFICATIONS APPEARING


	for i = 1, #Notifications do
		local Notification = Notifications[i]
		
		StarterGui:SetCore("SendNotification", {
			Title = "Done!",
			Text = Notification,
			Duration = 5
		})
		
		wait(TimeBetweenNotifications)
	end

end)

Section2:NewButton("Teleport Out Of The Bank", "ButtonInfo", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(5.0836553573608, 3.3980038166046, 233.85478210449)
end)

Section3:NewButton("Spawn Car", "", function()
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "PickingCar",
    [2] = "FordGT"
}

game:GetService("ReplicatedStorage").RemoteEvents.C5ar:FireServer(unpack(args))

local StarterGui = game:GetService("StarterGui")

local Notifications = {
	"Do not spam this button, or you will get kicked from the game!",
	
}

local TimeBetweenNotifications = 1 --REPLACE 5 WITH THE NUMBER OF SECONDS YOU WANT TO WAIT IN BETWEEN NOTIFICATIONS APPEARING


	for i = 1, #Notifications do
		local Notification = Notifications[i]
		
		StarterGui:SetCore("SendNotification", {
			Title = "Warning",
			Text = Notification,
			Duration = 5
		})
		
		wait(TimeBetweenNotifications)
	end

end)

Section1:NewSlider("Set WalkSpeed", "", 500, 16, function(s) -- 500 (MaxValue) | 16 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Section1:NewSlider("Set JumpPower", "", 500, 50, function(s) -- 500 (MaxValue) | 50 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

Section3:NewButton("Buy House", "", function()
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "BuyingHouse",
    [2] = workspace:FindFirstChild("001_Lots"):FindFirstChild("For Sale").Number.Number,
    [3] = workspace:FindFirstChild("001_Lots"):FindFirstChild("For Sale").BuyHouse,
    [4] = workspace:FindFirstChild("001_Lots"):FindFirstChild("For Sale").BuyHouse.PlotOfLand
}

game:GetService("ReplicatedStorage").RemoteEvents.G5ettingHouse:FireServer(unpack(args))

end)


end