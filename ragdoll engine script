local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Ragdoll Engine", "Sentinel")
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Scripts")



Section:NewButton("Anti-Push", ":troll:", function()
    game.Players.LocalPlayer.Character.Pushed:Destroy()
end)

Section:NewButton("Anti-Ragdoll", ":troll:", function()
    game.Players.LocalPlayer.Character.RagdollMe:Destroy()
end)