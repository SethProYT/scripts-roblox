local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local w = library:CreateWindow("Boxing League Unrealeased") -- Creates the window

local b = w:CreateFolder("Scripts") -- Creates the folder(U will put here your buttons,etc)

b:Label("Boxing League",{
    TextSize = 25; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
}) 

b:Toggle("No-Slow Legit",function(bool)
    shared.toggle = bool
    legitnoslow = (bool)
end)

b:Toggle("Very Fast No-Slow",function(bool)
    shared.toggle = bool
    hackerboinoslow = (bool)
end)


b:DestroyGui()

--[[
How to refresh a dropdown:
1)Create the dropdown and save it in a variable
local yourvariable = b:Dropdown("Hi",yourtable,function(a)
    print(a)
end)
2)Refresh it using the function
yourvariable:Refresh(yourtable)
How to refresh a label:
1)Create your label and save it in a variable
local yourvariable = b:Label("Pretty Useless NGL",{
    TextSize = 25; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255);
    BgColor = Color3.fromRGB(69,69,69);
})
2)Refresh it using the function
yourvariable:Refresh("Hello") It will only change the text ofc
]]





spawn(function()
    while wait() do
        if legitnoslow == true then
         game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        end
    end
end)

spawn(function()
    while wait() do
        if hackerboinoslow == true then
         game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 90
        end
    end
end)

spawn(function()
    while wait() do
        if hackerboinoslow == false then
         game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        end
    end
end)



