
Section1:NewToggle("Visualize Explorers", "", function(state)
    if state then
    visualizeExplorers()
    explorers = state
    else
        explorers = state
        local args = {
            [1] = "RolePlayBio",
            [2] = "Playing - Explorers"
        }
        
        game:GetService("ReplicatedStorage").RemoteEvents.R6PNameEvent:FireServer(unpack(args))
    
        local args = {
            [1] = "BusinessName",
            [2] = "Playing - Explorers"
        }
        
        game:GetService("ReplicatedStorage").RemoteEvents.RPHouseEvent:FireServer(unpack(args))
    
        local args = {
            [1] = "ReturningCommercialWords",
            [2] = 3,
            [4] = "Playing - Explorers"
        }
        
        game:GetService("ReplicatedStorage").RemoteEvents.C6emetery:FireServer(unpack(args))
        
        -- Script generated by R2Sv2
    -- R2Sv2 developed by Luckyxero
     
    local A_1 = "ReturningCommercialWords"
    local A_2 = 2
    local A_4 = "Playing - Explorers"
    local Event = game:GetService("ReplicatedStorage").RemoteEvents.C6emetery
    Event:FireServer(A_1, A_2, A_4)
    
    end
end)

Section1:NewToggle("Visualize Colbreakz", "", function(state)
    if state then
    visualizeColbreakz()
    colbreakz = state
    else
      colbreakz = state 
    end
end)

Section1:NewToggle("Visualize liaquo", "", function(state)
    if state then
        visualizeliaquo()
        liaquo = state 
    else
        liaquo = state 
    end
end)

Section1:NewToggle("Visualize at the speed of light", "", function(state)
    if state then
   visualizedimrain()
   dimrain = state
    else
    dimrain = state
    end
end)


Section1:NewButton("Tween to lobby", "", function()
   tweenservice()
end)





--//FUNCTIONS
---------------------------------------------

 function visualizeExplorers()
    spawn(function()
        while explorers == true do
        
            local args = {
                [1] = "RolePlayBio",
                [2] = "Hini nik"
            }
            
            game:GetService("ReplicatedStorage").RemoteEvents.R6PNameEvent:FireServer(unpack(args))
        
            wait(1)
               
            local args = {
                [1] = "RolePlayBio",
                [2] = "Explorers"
            }
            
            game:GetService("ReplicatedStorage").RemoteEvents.R6PNameEvent:FireServer(unpack(args))
        
            

            local args = {
                [1] = "BusinessName",
                [2] = "Hini nik"
            }
            
            game:GetService("ReplicatedStorage").RemoteEvents.RPHouseEvent:FireServer(unpack(args))

            wait(1)

            local args = {
                [1] = "BusinessName",
                [2] = "Explorers"
            }
            
            game:GetService("ReplicatedStorage").RemoteEvents.RPHouseEvent:FireServer(unpack(args))
        
            local args = {
                [1] = "ReturningCommercialWords",
                [2] = 3,
                [4] = "Hini nik"
            }
            
            game:GetService("ReplicatedStorage").RemoteEvents.C6emetery:FireServer(unpack(args))

            wait(1)
            
            local args = {
                [1] = "ReturningCommercialWords",
                [2] = 3,
                [4] = "Explorers"
            }
            
            game:GetService("ReplicatedStorage").RemoteEvents.C6emetery:FireServer(unpack(args))
            

         
        local A_1 = "ReturningCommercialWords"
        local A_2 = 2
        local A_4 = "Hini nik"
        local Event = game:GetService("ReplicatedStorage").RemoteEvents.C6emetery
        Event:FireServer(A_1, A_2, A_4)
        
        wait(1)

        local A_1 = "ReturningCommercialWords"
        local A_2 = 2
        local A_4 = "Explorers"
        local Event = game:GetService("ReplicatedStorage").RemoteEvents.C6emetery
        Event:FireServer(A_1, A_2, A_4)
        wait()
        end
    end)
end


