local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Brookhaven RP Visualizer", "Sentinel")

local Tab1 = Window:NewTab("Scooter")
local Section1 = Tab1:NewSection("Music")



Section1:NewButton("change to explorers", "", function()
    local args = {
        [1] = "RolePlayBio",
        [2] = "Playing - Explorers"
    }
    
    game:GetService("ReplicatedStorage").RemoteEvents.R6PNameEvent:FireServer(unpack(args))

    local args = {
        [1] = "PickingScooterMusicText",
        [2] = "1446376775"
    }
    
    game:GetService("ReplicatedStorage").RemoteEvents.NoMotorVehicles:FireServer(unpack(args))

    local args = {
        [1] = "BusinessName",
        [2] = "Playing - Explorers"
    }
    
    game:GetService("ReplicatedStorage").RemoteEvents.RPHouseEvent:FireServer(unpack(args))

    local args = {
        [1] = "ReturningCommercialWords",
        [2] = 3,
        [4] = "Playing - Explorers"
    }
    
    game:GetService("ReplicatedStorage").RemoteEvents.C6emetery:FireServer(unpack(args))
    
end)

