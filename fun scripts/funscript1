Section1:NewToggle("Visualize Explorers", "", function(state)
    if state then
      explorers = state
      visualizeExplorers()
    else
        explorers = state
    end
end)

Section1:NewToggle("Visualize colbreakz", "", function(state)
    if state then
      colbreakz = state
      visualizeColBreakz()
    else
        colbreakz = state
    end
end)

Section1:NewToggle("Visualize liaquo", "", function(state)
    if state then
      liaquo = state
      visualizeliaquo()
    else
        liaquo = state
    end
end)







function visualizeliaquo()
    spawn(function()
        while liaquo == true do
            local args = {
                [1] = "RolePlayBio",
                [2] = visualizerliaquo
            }
            
            game:GetService("ReplicatedStorage").RemoteEvents.R6PNameEvent:FireServer(unpack(args))
            wait()
        end
    end)
end









function visualizeColBreakz()
    spawn(function()
        while colbreakz == true do
            local args = {
                [1] = "RolePlayBio",
                [2] = visualizerColBreakz
            }
            
            game:GetService("ReplicatedStorage").RemoteEvents.R6PNameEvent:FireServer(unpack(args))
            wait()
        end
    end)
end





function visualizeExplorers()
    spawn(function()
        while explorers == true do
            local args = {
                [1] = "RolePlayBio",
                [2] = visualizerExplorers
            }
            
            game:GetService("ReplicatedStorage").RemoteEvents.R6PNameEvent:FireServer(unpack(args))
            wait()
        end
    end)
end










local visualizerExplorers = {
    "Hininik",
    "Explorers",
     
    }
    
    local visualizerColBreakz = {
       "ColBreakz",
       "2011",
    
    }
    
    local visualizerliaquo = {
        "lia quo",
        "Crystal Corruption",
        "(Nightcore Remix)",
    
    
    