    local TimeBetweenNotifications = 1 --REPLACE 5 WITH THE NUMBER OF SECONDS YOU WANT TO WAIT IN BETWEEN NOTIFICATIONS APPEARING
    
    
        for i = 1, #Notifications do
            local Notification = Notifications[i]
            
            StarterGui:SetCore("SendNotification", {
                Title = "Done!",
                Text = Notification,
                Duration = 5
            })
            
            wait(TimeBetweenNotifications)
        end
    
    end)
    
    Section2:NewButton("Teleport Out Of The Bank", "", function()
