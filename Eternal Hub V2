if game.placeId == 3738115442 then

    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Eternal Hub V2", "DarkTheme")
    local Tab1 = Window:NewTab("Main")
    local Section1 = Tab1:NewSection("Main")
    
    
    Section1:NewToggle("Legit No-Slow", "", function(state)
        if state then
        legit = state
        NoSlow()
        else
        legit = state 
        end
    end)

    Section1:NewToggle("Hacker Bo1 No-Slow", "", function(state)
        if state then
        nonlegit = state
        hacker()
        else
        nonlegit = state 
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        end
    end)




function NoSlow()
    spawn(function()
    while legit == true do
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        wait()
        end
    end)
end

function hacker()
    spawn(function()
        while nonlegit == true do
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 90
            wait()
        end
    end)
end







end


