local DiscordLib =
    loadstring(game:HttpGet "https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/discord")()

local win = DiscordLib:Window("Welcome back, "..game.Players.LocalPlayer.Name)

local serv = win:Server("Blox-Street Shopping", "")

local Raid = serv:Channel("Auto-Raid")

local Player = serv:Channel("Player")

local WalkSpeedSlider =
    Player:Slider(
    "Set WalkSpeed (no anti-cheat moment)",
    16,
    1000,
    16,
    function(t)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = t
    end
)
Player:Seperator()

local JumpPower =
    Player:Slider(
    "Set JumpPower (no anti-cheat moment)",
    16,
    1000,
    16,
    function(t)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = t
    end
)


Raid:Toggle("Auto-Raid", false, function(bool)
    getgenv().autoraid = bool
    raid()
end)

Raid:Seperator()

Raid:Button(
    "Chat Logs (credits to Lindsay on Verm)",
    function()
       loadstring(game:HttpGetAsync("https://pastebin.com/raw/Y8yW6Nas", true))()
    end
)

Raid:Seperator()

Raid:Button(
    "Remove chat logs",
    function()
        game:GetService("CoreGui").LogHolder:Destroy() 
    end
)

local serv2 = win:Server("Fun Scripts", "")

local FunScriptsChannel = serv2:Channel("Fun Scripts Server")


FunScriptsChannel:Button(
    "Remove all hats (fun)",
    function()
        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
             if v:IsA("Accessory") then
                 v:Destroy()
             end
        end
    end
)

