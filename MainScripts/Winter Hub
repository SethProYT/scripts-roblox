-- win:Server("Main", "http://www.roblox.com/asset/?id=7490438396")

function raid()
    spawn(function()
        while wait(1) do
            if getgenv().autoraid then
                for i, v in pairs(workspace:GetDescendants()) do
                    if v:IsA("ClickDetector") then
                        fireclickdetector(v)
                    end
                end
                game.Players.LocalPlayer.Character:Destroy()

                wait()
            end
        end
    end)
end

