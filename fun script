local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("fun", "Sentinel")
local Tab = Window:NewTab("fun")
local Section = Tab:NewSection("Scripts")

Section:NewDropdown("Set WalkSpeed To", "fun", {"16", "90",}, function(currentOption)
    local Path = game.Players.LocalPlayer.Character.Humanoid
    Path.WalkSpeed = (currentOption)
    if Path.WalkSpeed == 90 then
       -- Script generated by SimpleSpy - credits to exx#9394
    
    local args = {
        [1] = "Successfully Set WalkSpeed to 90!",
        [2] = "All"
    }
    
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
    end
    
    
    if Path.WalkSpeed == 16 then
      local args = {
        [1] = "Successfully Set WalkSpeed to Normal!",
        [2] = "All"
    }
    
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
    end
end)